{
  "extends": "@sushiswap/typescript-config/base.json",
  "include": ["src"],
  "exclude": ["dist", ".turbo", "node_modules"],
  "compilerOptions": {
    "jsx": "react-jsx",
    "lib": ["ES2017", "DOM"],
    "module": "ESNext",
    "target": "es6",
    "resolveJsonModule": true,
    "rootDir": "./src",
    "outDir": "./dist",
    "sourceMap": true,
    "declaration": true,
    "declarationMap": true
  }
}
